    "version": "0.2.0",
    "configurations": [
        {
            "name": "(lldb) Attach",
            "type": "cppdbg",
            "request": "attach",
            "program": "/Users/meghanj/Sandbox/anaconda-03/anaconda3/envs/pygmt-dev/bin/python",
            "processId": "${command:pickProcess}",
            "MIMode": "lldb"
        },
        {
            "type": "bashdb",
            "request": "launch",
            "name": "build release script",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/admin/build-release.sh",
            "args": []
        },
        {
            "type": "bashdb",
            "request": "launch",
            "name": "current shell script",
            "cwd": "${workspaceFolder}/vbuild/debug/",
            "program": "${file}",
            "args": []
        },
        {
            "name": "gmt begin",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/vbuild/src/gmt",
            "args": ["begin","test","png"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/vbuild/debug/",
            "environment": [],
            "envFile": "${workspaceFolder}/vbuild/debug/project.env",
            "externalConsole": false,
            "MIMode": "lldb"
        },
        {
            "name": "gmt command",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/vbuild/src/gmt",
            "args": [""],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}/vbuild/debug/",
            "environment": [],
            "envFile": "${workspaceFolder}/vbuild/debug/project.env",
            "externalConsole": false,
            "MIMode": "lldb"
        },
        {
            "name": "gmt basemap",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/vbuild/src/gmt",
            "args": ["basemap","-R20/50/20/35","-JC35/28/10c","-Bafg","-B+tCassini"],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}/vbuild/debug/",
            "environment": [],
            "envFile": "${workspaceFolder}/vbuild/debug/project.env",
            "externalConsole": false,
            "MIMode": "lldb"
        },
        {
            "name": "gmt end",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/vbuild/src/gmt",
            "args": ["end","show"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/vbuild/debug/",
            "environment": [],
            "envFile": "${workspaceFolder}/vbuild/debug/project.env",
            "externalConsole": false,
            "MIMode": "lldb"
        }
    ]
}