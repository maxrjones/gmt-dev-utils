    "version": "0.2.0",
    "configurations": [
        {
            "type": "bashdb",
            "request": "launch",
            "name": "current shell script",
            "cwd": "${workspaceFolder}/vbuild/debug",
            "program": "${file}",
            "args": []
        },
        {
            "name": "gmt version",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/vbuild/src/gmt.exe",
            "args": ["--version"],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}/vbuild/debug",
            "environment": [],
            "envFile": "${workspaceFolder}/.vscode/project.env",
            "externalConsole": false,
        },
        {
            "name": "gmt begin",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/vbuild/src/gmt.exe",
            "args": ["begin","test","png"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/vbuild/debug",
            "environment": [],
            "envFile": "${workspaceFolder}/.vscode/project.env",
            "externalConsole": false,
        },
        {
            "name": "gmt basemap",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/vbuild/src/gmt.exe",
            "args": ["basemap","-R20/50/20/35","-JC35/28/10c","-Bafg","-B+tCassini"],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}/vbuild/debug",
            "environment": [],
            "envFile": "${workspaceFolder}/.vscode/project.env",
            "externalConsole": false,
            "MIMode": "lldb"
        },
        {
            "name": "gmt end",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/vbuild/src/gmt.exe",
            "args": ["end","show"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/vbuild/debug",
            "environment": [],
            "envFile": "${workspaceFolder}/.vscode/project.env",
            "externalConsole": false,
            "MIMode": "lldb"
        },
        {
            "name": "gmt grdcut",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/vbuild/src/gmt.exe",
            "args": ["grdcut","@earth_relief_05m","-R15/115/15/70","-Gtest.nc"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/vbuild/debug",
            "environment": [],
            "envFile": "${workspaceFolder}/.vscode/project.env",
            "externalConsole": false,
            "MIMode": "lldb"
        }
    ]
}